<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - easydash</title>
    <base href="~/" />

    <link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/dist/app.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/dist/font-awesome/css/font-awesome.min.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/dist/bootswatch/darkly/bootstrap.min.css" asp-append-version="true" id="easy-swatch"/>
</head>
<body>
    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
